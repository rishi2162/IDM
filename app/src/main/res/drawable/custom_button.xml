<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true" android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="#0F0087D5"></solid>
            <corners android:radius="10dp"></corners>
            <stroke android:width="2dp" android:color="@color/incture_theme"></stroke>
        </shape>
    </item>

    <item android:state_enabled="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:padding="10dp" android:shape="rectangle">
            <!-- you can use any color you want I used here gray color-->
            <solid android:color="#0F0087D5" />
            <corners android:radius="10dp" />
            <stroke android:width="2dp" android:color="@color/btnReject_color"></stroke>
        </shape>
    </item>
</selector>
